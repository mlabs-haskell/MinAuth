    "@types/jsonwebtoken": "^9.0.3",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-strategy": "^0.2.35",
    "concurrently": "^8.2.2",
    "io-ts": "^2.2.20",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
